{
    "name": "envidea/laravel-sso",
    "description": "Simple PHP SSO integration for Laravel",
    "keywords": ["laravel", "sso", "authentication", "login"],
    "homepage": "https://github.com/danielalbiol/laravel-sso",
    "license": "Envidea",
    "authors": [{
        "name": "Envidea Multimedia",
        "email": "info@envidea.es",
        "homepage": "https://envidea.es",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "zefy/php-simple-sso": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Zefy\\LaravelSSO\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zefy\\LaravelSSO\\SSOServiceProvider"
            ]
        }
    }
}
